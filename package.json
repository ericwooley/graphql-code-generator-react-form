{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "watch": {
    "examples:generate": {
      "patterns": [
        "examples",
        "dist"
      ],
      "extensions": "gql,graphql,js",
      "quiet": false
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "concurrently --names 'NEXT,WATCH,BUILD' -c 'bgBlue.bold,bgMagenta.bold,bgRed.bold' 'yarn --silent examples:dev' 'npm run watch' 'yarn build --watch'",
    "predeploy:pages": " yarn --silent examples:build",
    "deploy:pages": "gh-pages -d examples/dist",
    "examples:generate": "graphql-codegen  --config ./examples/codegen.yml",
    "examples:dev": "cd examples && yarn dev",
    "examples:build": "cd examples && yarn build",
    "postinstall": "cd examples && yarn install",
    "watch": "npm-watch",
    "test": "jest"
  },
  "peerDependencies": {},
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "graphql-code-gen-react-forms",
  "author": "Eric Wooley",
  "module": "dist/graphql-code-gen-react-forms.esm.js",
  "devDependencies": {
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-typescript": "^7.13.0",
    "@graphql-codegen/cli": "^1.21.3",
    "@graphql-codegen/typescript": "^1.21.1",
    "@graphql-codegen/typescript-operations": "^1.17.15",
    "@types/jest": "^26.0.22",
    "@types/react": "^17.0.3",
    "concurrently": "^6.0.2",
    "gh-pages": "^3.1.0",
    "jest": "^26.6.3",
    "lz-string": "^1.4.4",
    "npm-watch": "^0.9.0",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@graphql-codegen/plugin-helpers": "^1.18.4",
    "@graphql-codegen/visitor-plugin-common": "^1.19.1",
    "auto-bind": "4.0.0",
    "change-case-all": "1.0.14",
    "graphql": "^15.5.0",
    "prettier": "^2.2.1"
  },
  "alias": {
    "react-dom": "@hot-loader/react-dom"
  }
}
